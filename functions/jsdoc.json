{
    "plugins": ["plugins/markdown"],
    "source": {
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|jsdocs)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
      
        "destination": "./jsdoc",         
        "recurse": true,
        "readme": "../README.md"   
    }
}